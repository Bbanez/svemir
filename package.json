{
  "name": "svemir",
  "version": "1.0.0-alpha1",
  "description": "Framework and tool set for Three.js",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "dev": "vite",
    "build:ts": "tsc -p .",
    "bundle": "npm run lint && npm-tool --bundle",
    "publish": "npm-tool --publish",
    "link": "npm-tool --link",
    "link:sudo": "npm-tool --link --sudo",
    "unlink": "npm-tool --unlink",
    "unlink:sudo": "npm-tool --unlink --sudo",
    "pack": "npm-tool --pack",
    "test": "mocha -r ts-node/register test/integration/*.test.ts --recursive --exit",
    "test:active": "mocha -r ts-node/register test/integration/*.active.test.ts --recursive --exit --bail",
    "test:unit": "mocha -r ts-node/register test/unit/*.test.ts --recursive --exit",
    "test:unit:active": "mocha -r ts-node/register test/unit/*.active.test.ts --recursive --exit",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bbanez/svemir.git"
  },
  "keywords": [
    "three",
    "three.js",
    "game",
    "engine"
  ],
  "author": "Branislav VajagiÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bbanez/svemir/issues"
  },
  "homepage": "https://github.com/Bbanez/svemir",
  "peerDependencies": {
    "axios": ">=0.25.0",
    "three": ">=0.137.5",
    "uuid": ">=8.3.2"
  },
  "devDependencies": {
    "@banez/fs": "1.0.2",
    "@banez/npm-tool": "1.1.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^9.1.0",
    "@types/node": "^15.6.2",
    "@types/three": "^0.137.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "axios": "^0.25.0",
    "chai": "^4.3.4",
    "eslint": "^7.9.0",
    "mocha": "^9.2.0",
    "prettier": "^2.2.1",
    "sass": "^1.49.7",
    "three": "^0.137.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "vite": "^2.7.13",
    "vue": "^3.2.25",
    "vue-tsc": "^0.31.1"
  }
}
